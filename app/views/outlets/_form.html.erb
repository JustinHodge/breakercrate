<%= form_with(model: outlet) do |form| %>
  <% if outlet.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(outlet.errors.count, "error") %> prohibited this outlet from being saved:</h2>

      <ul>
        <% outlet.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :type, style: "display: block" %>
    <%= form.text_field :type %>
  </div>

  <div>
    <%= form.label :is_gfci, style: "display: block" %>
    <%= form.check_box :is_gfci %>
  </div>

  <div>
    <%= form.label :room_id, style: "display: block" %>
    <%= form.text_field :room_id %>
  </div>

  <div>
    <%= form.label :integer, style: "display: block" %>
    <%= form.text_field :integer %>
  </div>

  <div>
    <%= form.label :breaker_id, style: "display: block" %>
    <%= form.number_field :breaker_id %>
  </div>

  <div>
    <%= form.label :location_x, style: "display: block" %>
    <%= form.number_field :location_x %>
  </div>

  <div>
    <%= form.label :location_y, style: "display: block" %>
    <%= form.number_field :location_y %>
  </div>

  <div>
    <%= form.label :location_z, style: "display: block" %>
    <%= form.number_field :location_z %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
